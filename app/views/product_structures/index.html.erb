<p id="notice"><%= notice %></p>

<h1>Product Structures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_structures.each do |product_structure| %>
      <tr>
        <td><%= link_to 'Show', product_structure %></td>
        <td><%= link_to 'Edit', edit_product_structure_path(product_structure) %></td>
        <td><%= link_to 'Destroy', product_structure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Structure', new_product_structure_path %>
