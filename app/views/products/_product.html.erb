<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="x_panel">
    <div class="x_title">
      <h2>Historia - Produktu <small> aktualne ceny</small></h2>
      <ul class="nav navbar-right panel_toolbox">
        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
        </li>
        <li><a class="close-link"><i class="fa fa-close"></i></a>
        </li>
      </ul>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
      <p class="text-muted font-13 m-b-30"></p>
      <table id="datatable-buttons" class="table table-striped table-bordered">
        <thead>
        <tr>
          <th>Nazwa</th>
          <th>Cena</th>
          <th class="col-md-2 center"><em class="fa fa-cog"></em></th>
        </tr>
        </thead>
        <tbody>
        <% obj.each do |product| %>
            <tr>
              <td><%= product.name %></td>
              <td><%= product.price %> z≈Ç</td>
              <td>
                <%= link_to(product_path(product), class: "glyphicon glyphicon-search") do %><% end %>
              </td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>



