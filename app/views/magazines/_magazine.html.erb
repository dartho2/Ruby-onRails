<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="x_panel">
    <div class="x_title">
      <h2>Magazyn <small>-</small></h2>
      <ul class="nav navbar-right panel_toolbox">
        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
        </li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
        </li>
        <li><a class="close-link"><i class="fa fa-close"></i></a>
        </li>
      </ul>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
      <p class="text-muted font-13 m-b-30">
        <%= link_to("Create New", new_magazine_path, class: "btn btn-success") %>  </p>
      <table id="datatable-buttons" class="table table-striped table-bordered">
        <thead>
        <tr>
          <th>Nazwa</th>
          <th>Ilość</th>
          <th>Cena Zakupu (E)</th>
          <th>CZ (F)</th>
          <th>KT (G)</th>
          <th>Marza (H)</th>
          <th>Z+KT (I)</th>
          <th>CS B (K)</th>
          <th>KS B (L)</th>
          <th>CS N (J)</th>
          <th>A K (M)</th>
          <th>Z N (N)</th>
          <th>Z B (O)</th>
          <th class="col-md-2 center"><em class="fa fa-cog"></em></th>
        </tr>
        </thead>

        <tbody>
        <% obj.each do |magazine| %>
            <tr>
              <td><%= magazine.productname %></td>
              <td><%= magazine.quantity %></td>
              <td><%= magazine.price %></td>
              <td><%= magazine.f %></td>
              <td><%= magazine.g(magazine.productshipment.weight) %></td>
              <td><%= magazine.h %></td>
              <td><%= magazine.i %></td>
              <td><%= magazine.k %></td>
              <td><%= magazine.l %></td>
              <td><%= magazine.j %></td>
              <td><%= magazine.m %></td>
              <td><%= magazine.n %></td>
              <td><%= magazine.o %></td>

              <td align="center">
                <%= link_to(magazine_path(magazine), class: 'glyphicon glyphicon-search') do %>
                <% end %>
                <%= link_to(edit_magazine_path(magazine), class: 'glyphicon glyphicon-edit') do %>
                <% end %>
              </td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>




