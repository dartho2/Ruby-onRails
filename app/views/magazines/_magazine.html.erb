<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="x_panel">
    <div class="x_title">
      <h2>Magazyn <small>-</small></h2>
      <ul class="nav navbar-right panel_toolbox">
        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
        </li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
        </li>
        <li><a class="close-link"><i class="fa fa-close"></i></a>
        </li>
      </ul>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
      <p class="text-muted font-13 m-b-30">
        <%= link_to("Create New", new_magazine_path, class: "btn btn-success") %>  </p>
      <table id="datatable-buttons" class="table table-striped table-bordered">
        <thead>
        <tr>
          <th>Nazwa</th>
          <th>Ilość</th>
          <th class="col-md-2 center"><em class="fa fa-cog"></em></th>
        </tr>
        </thead>


        <% obj.each do |magazine| %>
            <tbody>
            <tr>
              <td><%= magazine.productname %></td>
              <td><%= magazine.quantity %></td>

              <td align="center">
                <%= link_to(magazine_path(magazine), class: "btn btn-primary") do %>
                    <em class="fa fa-search"></em>
                <% end %>
                <%= link_to(edit_magazine_path(magazine), class: "btn btn-default") do %>
                    <em class="fa fa-pencil"></em>
                <% end %>
                <%= link_to(magazine_path(magazine), method: :delete,
                            data: {confirm: "Are u sure ?"},
                            class: "btn btn-danger") do %>
                    <em class="fa fa-trash"></em>
                <% end %>
              </td>
            </tr>
            </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>



