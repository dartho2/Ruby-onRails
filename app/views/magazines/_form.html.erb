<%= render '/shared/errors', obj: @magazine %>
<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <div class="panel panel-default panel-table">
        <div class="panel-heading">
          <div class="row">
            <div class="col col-xs-6">
              <h3 class="panel-title"></h3>
            </div>
            <div class="col-xs-12">
              <%= form_for(@magazine, :html => {class: "form-horizontal", role: "form"}) do |f| %>
                  <div class="form-group">
                    <div class="control-label col-sm-2">
                      <label for="product_category_id"></label><br/>
                    </div>
                    <div class="col-sm-2">
                    </div>
                    <div class="control-label col-sm-2 center">
                      <div class="center"></div>
                    </div>
                    <div class="col-sm-4">
                        </div>
                  </div>
                  <div class="form-group">
                    <div class="control-label col-sm-2">
                      <%= f.label :productname, "Product Name" %>
                    </div>
                    <div class="col-sm-8">
                      <%= f.text_field :productname, class: "form-control", placeholder: "Name", autofocus: true %>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="control-label col-sm-2">
                      <%= f.label :quantity %>
                    </div>
                    <div class="col-sm-8">
                      <%= f.text_field :quantity, class: "form-control", placeholder: "Quantity" %>
                    </div>
                  </div>
                  <div class="form-group center">
                    <div class="col-sm-6">
                      <%= f.submit class: "btn  btn-success" %>
                    </div>
                    <div class="col-sm-2">

                    </div>
                  </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

